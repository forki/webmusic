{
    "compileOnSave": false,
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "sourceMap": true,
        "target": "es5"
    },
    "files": [
        "actions.ts",
        "app.ts",
        "defines.ts",
        "typings/index.d.ts",
        "jsx/index.tsx"
    ],
    "filesGlob": [
        "*.ts",
        "typings/index.d.ts",
        "app.ts",
        "jsx/*ts*"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
