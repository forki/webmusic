{
    "compileOnSave": false,
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "sourceMap": true,
        "experimentalDecorators": true,
        "target": "es6",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "outDir": "out"
    },
    "exclude": [
        "node_modules",
        "fsharp",
        "transcoded_tracks"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
