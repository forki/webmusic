{
    "compileOnSave": false,
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "sourceMap": true,
        "target": "es5",
        "outDir": "out"
    },
    "files": [
        "actions.ts",
        "app.ts",
        "defines.ts",
        "server.ts",
        "typings/index.d.ts",
        "jsx/config.tsx",
        "jsx/index.tsx",
        "jsx/library.tsx",
        "jsx/playlist.tsx",
        "lib/archive.org.ts",
        "scripts/archive.org.ts"
    ],
    "filesGlob": [
        "*.ts",
        "typings/index.d.ts",
        "app.ts",
        "jsx/*ts*",
        "lib/*ts*",
        "scripts/*ts*"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
